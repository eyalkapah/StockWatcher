<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="clr-namespace:StockWatcher.Converters">
    <converters:GeneralToVisibilityConverter x:Key="GeneralToVisibilityConverter" />
    <converters:TrendColorConverter x:Key="TrendColorConverter" />
    <converters:NullToVisibilityConverter x:Key="NullToVisibilityConverter" />
    <converters:NullToVisibilityConverter x:Key="NullToVisibilityInverseConverter" Inverse="True" />
    <converters:StringToResourceConverter x:Key="StringToResourceConverter" />
    <converters:BooleanInverseConverter x:Key="BooleanInverseConverter" />
</ResourceDictionary>